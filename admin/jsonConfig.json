{
    "i18n": true,
    "type": "tabs",
    "tabsStyle": {
        "width": "calc(100% - 100px)"
    },
    "items": {
        "_options": {
            "type": "panel",
            "label": "Options",
            "items": {
                "daysPreview": {
                    "newLine": true,
                    "type": "number",
                    "min": 0,
                    "step": 1,
                    "xs": 12,
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "Preview days"
                },
                "daysPast": {
                    "type": "number",
                    "min": 0,
                    "step": 1,
                    "xs": 12,
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "Past days"
                },
                "ignoreCaseInEventname": {
                    "type": "checkbox",
                    "newLine": true,
                    "xs": 12,
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "Ignore case for event-search"
                },
                "exactMatchInEventname": {
                    "type": "checkbox",
                    "newLine": true,
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "Exact Match for event-search"
                }
            }
        },
        "_html": {
            "type": "panel",
            "label": "HTML settings",
            "items": {
                "language": {
                    "type": "select",
                    "xs": 12,
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "options": [
                        {
                            "label": "English",
                            "value": "en"
                        },
                        {
                            "label": "Deutsch",
                            "value": "de"
                        },
                        {
                            "label": "русский",
                            "value": "ru"
                        },
                        {
                            "label": "Portugues",
                            "value": "pt"
                        },
                        {
                            "label": "Nederlands",
                            "value": "nl"
                        },
                        {
                            "label": "français",
                            "value": "fr"
                        },
                        {
                            "label": "Italiano",
                            "value": "it"
                        },
                        {
                            "label": "Espanol",
                            "value": "es"
                        }
                    ],
                    "default": "en",
                    "label": "Language"
                },
                "fulltime": {
                    "type": "text",
                    "newLine": true,
                    "xs": 12,
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "Use by full day"
                },
                "defColor": {
                    "type": "color",
                    "xs": 12,
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "Default color for HTML"
                },
                "dataPaddingWithZeros": {
                    "type": "checkbox",
                    "newLine": true,
                    "xs": 12,
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "Padding date with '0'"
                },
                "replaceDates": {
                    "type": "checkbox",
                    "xs": 12,
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "Replace dates with words"
                },
                "everyCalOneColor": {
                    "type": "checkbox",
                    "newLine": true,
                    "xs": 12,
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "Every calendar has own color"
                },
                "colorize": {
                    "type": "checkbox",
                    "xs": 12,
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "Colorize HTML"
                },
                "forceFullday": {
                    "type": "checkbox",
                    "newLine": true,
                    "xs": 12,
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "Hide Start-End of events"
                },
                "hideYear": {
                    "type": "checkbox",
                    "xs": 12,
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "Hide Year"
                },
                "arrowAlreadyStarted": {
                    "type": "checkbox",
                    "newLine": true,
                    "xs": 12,
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "Add arrow for started events"
                },
                "addColorBox": {
                    "type": "checkbox",
                    "xs": 12,
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "Add event color if available"
                }
            }
        },
        "_calendars": {
            "type": "panel",
            "label": "Calendars",
            "items": {
                "calendars": {
                    "type": "table",
                    "xs": 12,
                    "sm": 12,
                    "md": 12,
                    "lg": 12,
                    "label": "Calendars",
                    "items": [
                        {
                            "type": "text",
                            "attr": "name",
                            "width": "15%",
                            "title": {
                                "en": "Name",
                                "de": "Name",
                                "ru": "Имя",
                                "pt": "Nome",
                                "nl": "Naam",
                                "fr": "Nom",
                                "it": "Nome",
                                "es": "Nombre",
                                "pl": "Nazwa",
                                "uk": "Ім'я",
                                "zh-cn": "姓名"
                            },
                            "filter": true,
                            "sort": true,
                            "default": "My Calendar"
                        },
                        {
                            "type": "text",
                            "attr": "url",
                            "width": "20%",
                            "title": {
                                "en": "Calendar URL or file name",
                                "de": "Kalender URL oder Dateiname",
                                "ru": "Календарь URL или имя файла",
                                "pt": "Calendário URL ou nome de arquivo",
                                "nl": "Calendar URL of dossier naam",
                                "fr": "URL de calendrier ou nom de fichier",
                                "it": "URL del calendario o nome del file",
                                "es": "Calendario URL o nombre de archivo",
                                "pl": "Calendar URL lub nazwy plików",
                                "uk": "URL-адреса календара або назва файлу",
                                "zh-cn": "卡伦·URL或档案"
                            },
                            "filter": false,
                            "sort": false,
                            "default": ""
                        },
                        {
                            "type": "text",
                            "attr": "user",
                            "width": "10%",
                            "title": {
                                "en": "User name",
                                "de": "Benutzername",
                                "ru": "Имя пользователя",
                                "pt": "Nome do utilizador",
                                "nl": "Noem",
                                "fr": "Nom de l ' utilisateur",
                                "it": "Nome utente",
                                "es": "Nombre de usuario",
                                "pl": "Imię User",
                                "uk": "Ім'я користувача",
                                "zh-cn": "使用名称"
                            },
                            "filter": false,
                            "sort": false,
                            "default": false
                        },
                        {
                            "type": "password",
                            "repeat": false,
                            "visible": false,
                            "attr": "pass",
                            "width": "10%",
                            "title": {
                                "en": "Password",
                                "de": "Passwort",
                                "ru": "Пароль",
                                "pt": "Senha",
                                "nl": "Wachtwoord",
                                "fr": "Mot de passe",
                                "it": "Password",
                                "es": "Contraseña",
                                "pl": "Password",
                                "uk": "Логін",
                                "zh-cn": "护照"
                            },
                            "filter": false,
                            "sort": false,
                            "default": false
                        },
                        {
                            "type": "checkbox",
                            "attr": "sslignore",
                            "width": "10%",
                            "title": {
                                "en": "Ignore SSL errors",
                                "de": "SSL-Fehler ignorieren",
                                "ru": "Игнорировать ошибки SSL",
                                "pt": "Ignorar erros SSL",
                                "nl": "Negeer SSL fouten",
                                "fr": "Ignore les erreurs SSL",
                                "it": "Ignora errori SSL",
                                "es": "Ignorar errores SSL",
                                "pl": "Ignore SSL error",
                                "uk": "Ignore SSL помилки",
                                "zh-cn": "Ignore SSL错误"
                            },
                            "filter": false,
                            "sort": false,
                            "default": false
                        },
                        {
                            "type": "color",
                            "attr": "color",
                            "width": "10%",
                            "title": {
                                "en": "Color",
                                "de": "Farbe",
                                "ru": "Цвет",
                                "pt": "Cor",
                                "nl": "Kleur",
                                "fr": "Couleur",
                                "it": "Colore",
                                "es": "Color",
                                "pl": "Kolor",
                                "uk": "Колір",
                                "zh-cn": "颜色"
                            },
                            "filter": false,
                            "sort": false,
                            "default": "#000000"
                        },
                        {
                            "type": "text",
                            "attr": "filter",
                            "width": "15%",
                            "title": {
                                "en": "Filter",
                                "de": "Filter",
                                "ru": "Фильтр",
                                "pt": "Filtro",
                                "nl": "Filter",
                                "fr": "Filtre",
                                "it": "Filtro",
                                "es": "Filtro",
                                "pl": "Filter",
                                "uk": "Фільтри",
                                "zh-cn": "电影"
                            },
                            "filter": false,
                            "sort": false,
                            "default": "ignored events"
                        },
                        {
                            "type": "checkbox",
                            "attr": "filterregex",
                            "width": "10%",
                            "title": {
                                "en": "Filter as regular expression",
                                "de": "Filter als regulärer Ausdruck",
                                "ru": "Фильтр как регулярное выражение",
                                "pt": "Filtrar como expressão regular",
                                "nl": "Filter als regelmatige uitdrukking",
                                "fr": "Filtrer comme expression régulière",
                                "it": "Filtra come espressione normale",
                                "es": "Filtro como expresión regular",
                                "pl": "Filter jako zwykły wyraz wyrażenia",
                                "uk": "Фільтр як регулярний вираз",
                                "zh-cn": "过滤为正则表达式"
                            },
                            "filter": false,
                            "sort": false,
                            "default": false
                        }
                    ]
                },
                "customUserAgentEnabled": {
                    "type": "checkbox",
                    "newLine": true,
                    "xs": 12,
                    "sm": 12,
                    "md": 4,
                    "lg": 4,
                    "label": "Use custom user agent"
                },
                "customUserAgent": {
                    "hidden": "!data.customUserAgentEnabled",
                    "type": "text",
                    "xs": 12,
                    "sm": 12,
                    "md": 8,
                    "lg": 8,
                    "label": "User agent HTTP header"
                }
            }
        },
        "_events": {
            "type": "panel",
            "label": "Events",
            "items": {
                "events": {
                    "type": "table",
                    "xs": 12,
